{% trans_default_domain 'edu_travel_route' %}
    <div class="modal fade" id="new_travel_route" role="dialog" aria-labelledby="new-person-label" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="new-person-label">{{ 'title.new'|trans }}</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    {{ render(controller('AppBundle\\Controller\\Organization\\TravelRouteAPIController::apiNewTravelRouteAction')) }}
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">{{ 'form.new_travel_route.close'|trans }}</button>
                    <button type="button" class="btn btn-success" id="create_travelRoute">{{ 'form.new_travel_route.save'|trans }}</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
